<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>method</key>
    <string>developer-id</string>
    <key>teamID</key>
    <string>YourTeamID</string>  <!-- Replace with your actual team ID -->
    <key>signingStyle</key>
    <string>manual</string>
    <key>provisioningProfiles</key>
    <dict>
        <key>com.yourcompany.yourapp</key>  <!-- Replace with your app's bundle identifier -->
        <string>YourProvisioningProfile</string>  <!-- Replace with your provisioning profile name -->
    </dict>
    <key>signingCertificate</key>
    <string>Developer ID Application</string>
</dict>
</plist>
